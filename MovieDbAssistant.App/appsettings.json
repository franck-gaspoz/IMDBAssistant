{
  /* logging */

  "Logging": {
    "LogLevel": {
      "Default": "Debug"
    }
  },

  /* app */

  "App": {
    "Title": "Movie Db Assistant"
  },

  "BalloonTips": {
    "Start": "Running",
    "End": "Good bye !",
    "Delay": 4000
  },

  "Texts": {
    "Exit": "Exit",
    "Help": "Help (GitHub)",
    "OpenCmdLine": "Open command line",
    "OpenOutpFolder": "Open output folder",
    "OpenInpFolder": "Open input folder",
    "Settings": "Settings ...",
    "BuildFromInputFolder": "Build from input folder",
    "BuildFromQueryFile": "Build from query file...",
    "BuildFromJsonFile": "Build from Json file...",
    "BuildFromClipboard": "Build from clipboard (query)",
    "ProcInpFold": "processing input folder"
  },

  "Errors": {
    "BackgroundWorkerWrapper_Not_Initialized": "call Setup first"
  },

  "Assets": {
    "Icons": {
      "Tray": "multimedia.ico",
      "Buzy1": "led-red.ico",
      "Buzy2": "led-black.ico"
    }
  },

  "AssetsPath": "assets",

  "Shell": {
    "CommandLine": "cmd.exe",
    "Args": "/K \"dir *.exe\""
  },

  "FolderExplorer": {
    "CommandLine": "explorer.exe"
  },

  "OpenBrowser": {
    "CommandLine": "explorer.exe"
  },

  "Urls": {
    "HelpGitHub": "https://github.com/franck-gaspoz/MovieDbAssistant/blob/main/README.md"
  },

  "Paths": {
    "Output": "output",
    "Input": "input"
  },

  "Build": {
    "SearchPatternJson": "*.json",
    "SearchPatternTxt": "*.txt",
    "PrefixFileDisabled": "-"
  },

  "Anims": {
    "Interval": {
      "Dot": 500,
      "WaitTrayIcon": 250
    },
    "WaitIcons": [
      "led-red.ico",
      "led-black.ico"
    ]
  }
}
