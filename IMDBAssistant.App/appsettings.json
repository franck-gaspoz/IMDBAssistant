{
  /* logging */

  "Logging": {
    "LogLevel": {
      "Default": "Debug"
    }
  },

  /* app */

  "App": {
    "Title": "IMDb Assistant"
  },

  "BalloonTips": {
    "Start": "Running",
    "End": "Good bye !",
    "Delay": 4000
  },

  "Texts": {
    "Exit": "Exit",
    "Help": "Help (GitHub)",
    "OpenCmdLine": "Open command line",
    "OpenOutpFolder": "Open output folder",
    "OpenInpFolder": "Open input folder",
    "Settings": "Settings ...",
    "BuildFromInputFolder": "Build from input folder",
    "BuildFromQueryFile": "Build from query file...",
    "BuildFromJsonFile": "Build from Json file...",
    "BuildFromClipboard": "Build from clipboard (query)",
    "ProcInpFold": "processing input folder ..."
  },

  // <a href="https://www.flaticon.com/free-icons/movie-theater" title="movie theater icons">Movie theater icons created by Freepik - Flaticon</a>
  "IconFile": "multimedia.ico",

  "AssetsPath": "assets",

  "Shell": {
    "CommandLine": "cmd.exe",
    "Args": "/K \"dir *.exe\""
  },

  "FolderExplorer": {
    "CommandLine": "explorer.exe"
  },

  "OpenBrowser": {
    "CommandLine": "explorer.exe"
  },

  "Urls": {
    "HelpGitHub": "https://github.com/franck-gaspoz/IMDBAssistant/blob/main/README.md"
  },

  "Paths": {
    "Output": "output",
    "Input": "input"
  },

  "Build": {
    "SearchPatternJson": "*.json",
    "SearchPatternTxt": "*.txt",
    "PrefixFileDisabled": "-"
  },

  "Anims": {
    "Interval": {
      "Dot": 500
    }
  }
}
